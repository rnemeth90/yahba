package util

// Must panics on non-nil errors. Useful to handling programmer level errors.
func Must(err error) {
	if err != nil {
		panic(err)
	}
}

func HandleError(err error) {
	if err == nil {
		return
	}

	handleError(context.Background(), err, "unhandled error")
}

